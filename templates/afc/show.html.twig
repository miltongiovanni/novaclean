{% extends 'base.html.twig' %}

{% block title %}Afc{% endblock %}

{% block body %}
    <h1>Afc</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ afc.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ afc.nombre }}</td>
            </tr>
            <tr>
                <th>Contacto</th>
                <td>{{ afc.contacto }}</td>
            </tr>
            <tr>
                <th>Telefono</th>
                <td>{{ afc.telefono }}</td>
            </tr>
            <tr>
                <th>Extension</th>
                <td>{{ afc.extension }}</td>
            </tr>
            <tr>
                <th>Fecha_creation</th>
                <td>{{ afc.fechaCreation ? afc.fechaCreation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Fecha_actualizacion</th>
                <td>{{ afc.fechaActualizacion ? afc.fechaActualizacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Celular</th>
                <td>{{ afc.celular }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_afc_index') }}">back to list</a>

    <a href="{{ path('app_afc_edit', {'id': afc.id}) }}">edit</a>

    {{ include('afc/_delete_form.html.twig') }}
{% endblock %}
